# Tiz Staging Infrastructure Environment Variables
# 复制此文件为 .env 并修改为实际值

# 数据存储路径
DATA_PATH=/opt/tiz/data

# MySQL
MYSQL_ROOT_PASSWORD=<your-secure-password>
MYSQL_PASSWORD=<your-secure-password>

# Redis
REDIS_PASSWORD=<your-secure-password>

# Elasticsearch
ELASTIC_PASSWORD=<your-secure-password>

# Nacos
NACOS_AUTH_TOKEN=<your-secure-token-at-least-32-characters>

# Kafka
KAFKA_CLUSTER_ID=<unique-cluster-id>
KAFKA_HOST=<your-server-host-or-ip>
